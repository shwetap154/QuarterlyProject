        function openAddAAPCallObjScreen() {
            window.open('/apex/ZTS_EU_AddAAPCallObjectives?callId=' +
                '{!call.id}' + '&accountid=' + '{!call.ZTS_EU_Account__c}' +
                '&searchString=' + '', '_blank',
                'width=1000,height=520,top=100, left=255",resizable=0,scrollbars=1'
            );
            return false;
        }

        // Code added by Shreyas for showing Sample Drop Pop-up Page.
        function openSampleDropScreen() {
            window.open('/apex/ZTS_EU_AddSampleDrops?callId=' + '{!call.id}',
                '_blank',
                'width=1150,height=520,top=100, left=180",resizable=0,scrollbars=1'
            );
            return false;
        }

        // Code added by Shreyas for showing Gift Pop-up Page.
        function openGiftScreen(contactId) {
            window.open('/apex/ZTS_EU_AddGiftFromCall?callId=' + '{!call.id}' +
                '&contactId=' + contactId, '_blank',
                'width=1190,height=520,top=100, left=100",resizable=0,scrollbars=1'
            );
            return false;
        }

        function showKeyMessages(containerId, msg_ele) {
            //alert(document.getElementById(containerId));
            var keymsg_div = document.getElementById(containerId);
            var display = keymsg_div.style.display;
            if (display == 'none') {
                msg_ele.innerHTML = 'Hide';
                keymsg_div.style.display = '';
            } else {
                msg_ele.innerHTML = 'Show';
                keymsg_div.style.display = 'none';
            }
        }
        
        function showPopup() {
            var ErrorMessage = '';
            let errorElement = document.getElementById('pgid:formID:sErrorMsg');
            displayPopup = true;      
            let submitButton = document.getElementById('pgid:formID:submitButton');
            if({!shwSubmitError}){
                ErrorMessage ='{!$Label.ZTS_EU_Call_cannot_be_submitted_prior_to_the_activity_date}';
                submitButton.classList.add('hide');
            }else if({!showErr}){
                ErrorMessage = '{!$Label.ZTS_EU_You_would_not_be_able_to_make_any_more_changes_14_days_after_a_call_is_su}';
            }else{
                ErrorMessage = 'Confirm Save';
            }
            let containerObj = document.getElementById('pgid:formID:confirmPopup');
            containerObj.classList.remove('hide');             
            errorElement.innerHTML = ErrorMessage;     
        }

        function hidePopup() {
            let containerObj = document.getElementById('pgid:formID:confirmPopup');
            containerObj.classList.add('hide');
            let submitButton = document.getElementById('pgid:formID:submitButton');
            submitButton.classList.remove('hide');
        }

        function processSubmit(){
            var processingMessage = 'Please wait, your call is being Saved...';
            let elementObj = document.getElementById('pgid:formID:sErrorMsg');
            let surveyObj = document.getElementById('pgid:formID:surveyPrompt');
            let submitButton = document.getElementById('pgid:formID:submitButton');
            let cancelButton = document.getElementById('pgid:formID:cancelButton');
            elementObj.innerHTML = processingMessage;
            surveyObj.classList.add('hide');
            submitButton.classList.add('hide');
            cancelButton.classList.add('hide');
        }
