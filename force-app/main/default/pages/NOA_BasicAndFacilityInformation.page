                                                    <div class="requiredInput">
                                                        <div class="requiredBlock"></div>
                                                        <apex:inputText value="{!newOnlineAcc.Tax_Permit__c}" label=""   />
                                                    </div>
                                                </apex:outputPanel> 
                                                </apex:pageBlockSectionItem>
                                                
                                              </apex:pageBlockSection>
                                              </apex:panelGroup>
                                              </apex:panelGroup> 
                                </apex:pageBlockSection>
                                
