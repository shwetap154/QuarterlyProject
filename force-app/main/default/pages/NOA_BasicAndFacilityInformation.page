                                                        <div class="requiredInput">
                                                            <div class="requiredBlock"></div>
                                                            <apex:inputField value="{!newOnlineAcc.Existing_Account_Name__c}" label="" 
                                                                             onkeypress="return noenter(event);"
                                                                             />
