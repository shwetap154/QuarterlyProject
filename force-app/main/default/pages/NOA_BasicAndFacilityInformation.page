                                                            <div class="requiredBlock"></div>
                                                            <apex:inputField value="{!newOnlineAcc.Shipping_City__c}" label=""   onkeypress="return noenter(event);" />
                                                        </div>
                                                    </apex:outputPanel>
                                                </apex:pageBlockSectionItem>
                                                
                                                <apex:pageBlockSectionItem >
                                                    <label>State</label>
