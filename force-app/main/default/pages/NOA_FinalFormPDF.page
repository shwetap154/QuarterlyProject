<apex:page standardController="Lead" applyHtmlTag="false" applyBodyTag="false"  extensions="NOA_NewOnlineAccount" standardStylesheets="false" 
           showHeader="false" renderAs="pdf" >
    
    <head>
        <style type="text/css">
            @page {  
            @bottom-right {
            content: element(footer); 
            }
            }
            div.footer {
            margin-left:640px;
            display: block;
            padding: 5px;
            position: running(footer); 
            }      
            .headerRow .headerStyle{
            background-color:#ADD8E6;
            color:#000000;
            text-align:center;
            } 
            .tableClass tbody{
            text-align:center;
            }
            body,td {                
            font-size: 13px;
            font-family: Arial,Helvetica,sans-serif;
            } 
            td { 
            padding: 2px;
            }
            .bPageBlock .detailList  {
            padding: 2px 2px 2px 2px;
            
            width: 100%;
            font-family: Arial,Helvetica,sans-serif;
            } 
            .pageBlockTable {
            font-size:11.5px; 
            font-family: Arial,Helvetica,sans-serif;
            page-break-after:always;
            }
            .pageBlockTableHeader {
            font-size:9.5px;
            border-color: white;
            font-family: Arial,Helvetica,sans-serif;
            }
            h1, h2, h3
            {
            margin: 2px;
            padding: 2px;
            color: white;
            background-color:#2f94bc;            
            } 
            .headers  {
            color: white;
            text-align: center;
            position: relative;
            text-transform: uppercase;
            background-color: #6dd2de;
            padding: 0.5em 0em 0.5em;
            font-family: "proxima-nova-extra-condensed", Helvetica, Arial, sans-serif;
            
            font-size: 1.75em;
            line-height: 1.5;
            width:100%;
            float:left;
            } 
            
            .esignTagSize{
                font-size: 2em;
                font-weight: bold;
            }
            
            .esignVetTagSize{
                font-size: 12pt; 
                
            }
            
            .appheader  {
                color: white;
                text-align: center; 
                text-transform: uppercase;
                background-color: #6dd2de; 
                padding: 0.5em 0em 0.5em;
                font-family: Arial,Helvetica,sans-serif;
                font-size: 1.75em;
                line-height: 1.5;
                width:100%;
                float:left;
            }
            
            .appid { 
                font-size: 0.75em ; 
            }
            
            .spanapp {
                font-weight: 200;
                font-family: Arial,Helvetica,sans-serif !important;
            }  
            
        </style>
        
    </head>
    <div class="footer">
        <span>  <apex:image value="{! $Resource.ZoetisLogo}"   />       </span>
    </div>
    <div> 
        
    <apex:image url="{!URLFOR($Resource.NewOnlineAccount, 'images/ZoetisBanner_left.jpg')}" alt="Zoetis" width="100%" />         
    <apex:image url="{!URLFOR($Resource.NewOnlineAccount, 'images/ZoetisSpecies.png')}" alt="Zoetis" width="100%" />  
        
    <div class="appheader">  
        <apex:outputText value="<b>New Account Application</b> " escape="false" />  
        <br/>
        <apex:outputText styleClass="appid" value="Application Number: {!newOnlineAcc.NOA_Application_Number__c}" rendered="{!newOnlineAcc.NOA_Application_Number__c != null}" />
    </div> 
    <!-- <c:NOA_header noaLeadObj="{!newOnlineAcc}" ></c:NOA_header> -->
    </div>
    <br/><br/><br/><br/><br/><br/><br/>
    <apex:form style="width :100%">
        <center>
            <apex:outputPanel id="pdfForm" style="text-align: left;" layout="block">
                <apex:pageBlock > 
                    <apex:pageBlockSection rendered="{!!downloadFlag}">
                        <apex:outputText value="<br/><br/>" escape="false"  />
                    </apex:pageBlockSection>
                    <apex:outputPanel rendered="{!formType == 'Basic' }" >
                        <apex:pageBlockSection title="{! IF(newOnlineAcc.New_Shipping_Location__c,'Applicant Information' ,'Applicant & Facility Information')}" columns="1"  >
                            <apex:outputField value="{!newOnlineAcc.Salutation}" label="Applicant Prefix" />
                            <apex:outputField value="{!newOnlineAcc.FirstName}" label="Applicant First Name"/>
                            <apex:outputField value="{!newOnlineAcc.LastName}" label="Applicant Last Name"/>
                             <apex:outputField value="{!newOnlineAcc.Applicant_Role__c}" label="Applicant Role"/>
                            <apex:outputField value="{!newOnlineAcc.Email}" label="Applicant Email"/>
                            <apex:outputField value="{!newOnlineAcc.Phone}" label="Applicant Phone"/>                            
                            <apex:outputField value="{!newOnlineAcc.SAP_Bill_To_ID__c}" label="Bill To ID#"/>
                            <apex:outputField value="{!newOnlineAcc.Business_Name__c}" label="Business Name"  />
                            <apex:outputField value="{!newOnlineAcc.Customer_Type__c}" label="Customer Type" rendered="{!!newOnlineAcc.New_Shipping_Location__c}" />
                            <apex:outputField value="{!newOnlineAcc.Customer_Sub_Type__c}" label="Customer Sub Type" rendered="{!!newOnlineAcc.New_Shipping_Location__c}" />
                            <apex:pageBlockSectionItem rendered="{!!newOnlineAcc.New_Shipping_Location__c && newOnlineAcc.Customer_Type__c == 'Other' }"  >
                                <label style="color:#4A4A56;" for="other" ><b>If other, please describe:</b></label>
                                <apex:outputField id="other" label="" value="{!newOnlineAcc.Customer_Type_Other__c}" />
                            </apex:pageBlockSectionItem>
                            <apex:outputField value="{!newOnlineAcc.Total_number_of_practitioners__c}" label="Total number of practitioners at this location" rendered="{!!newOnlineAcc.New_Shipping_Location__c}" />
                        </apex:pageBlockSection> 
                        
                        <apex:pageBlockSection title="Business Segment" columns="2">
                            <apex:pageBlockSection columns="1">
                                <apex:outputText label="Small Animal" value="{!IF(newOnlineAcc.Small_Animal__c != null, TEXT(newOnlineAcc.Small_Animal__c) + ' %', '')}" />
                                <apex:outputText label="Equine" value="{!IF(newOnlineAcc.Equine__c != null,  TEXT(newOnlineAcc.Equine__c) + ' %', '')}"/>
                                <apex:outputText label="Beef" value="{!IF(newOnlineAcc.Beef__c != null, TEXT(newOnlineAcc.Beef__c) + ' %', '')}"/>
                                <apex:outputText label="Dairy" value="{!IF(newOnlineAcc.Dairy__c != null, TEXT(newOnlineAcc.Dairy__c) + ' %', '')}"/>
                                <apex:outputText label="Swine" value="{!IF(newOnlineAcc.Swine__c != null, TEXT(newOnlineAcc.Swine__c) + ' %', '')}"/> 
                            </apex:pageBlockSection>
                            <apex:pageBlockSection columns="1" >
                                <apex:outputText label="Feed" value="{!IF(newOnlineAcc.Feed__c != null, TEXT(newOnlineAcc.Feed__c) + ' %', '')} "/> 
                                <apex:outputText label="Poultry" value="{!IF(newOnlineAcc.Poultry__c != null, TEXT(newOnlineAcc.Poultry__c) + ' %', '')} "/> 
                                <apex:outputText label="Eggs" value="{!IF(newOnlineAcc.Eggs__c != null, TEXT(newOnlineAcc.Eggs__c) + ' %', '')} " rendered="{!newOnlineAcc.Poultry__c != null}" />
                                <apex:outputText label="Turkey" value="{!IF(newOnlineAcc.Turkey__c != null, TEXT(newOnlineAcc.Turkey__c) + ' %', '')} " rendered="{!newOnlineAcc.Poultry__c != null}" />
                                <apex:outputText label="Broiler" value="{!IF(newOnlineAcc.Broiler__c != null, TEXT(newOnlineAcc.Broiler__c) + ' %', '')} " rendered="{!newOnlineAcc.Poultry__c != null}" /> 
                            </apex:pageBlockSection>                         
                        </apex:pageBlockSection>
                        <!-- Specialty hospital section added for SC-006354 by C. Paice-->
                        <apex:pageBlockSection title="Hospital Type" columns="1">
                            <apex:outputField value="{!newOnlineAcc.Specialty_Hospital__c}" label="Specialty Hospital"  />
                            <apex:outputField value="{!newOnlineAcc.Specialty_Description__c}" label="Specialty Hospital Type"  />
                        </apex:pageBlockSection>
                        <apex:pageBlocksection title="Billing Address" columns="2"  >
                            <apex:pageBlocksection columns="1" >
                                <apex:outputText value="<font color=#4a4a56><b> I authorize Zoetis to send Invoices and Statements via: </b></font> {!newOnlineAcc.Contact_Preference__c}" escape="false" rendered="{!!newOnlineAcc.New_Shipping_Location__c}" />
                                <apex:outputField label="Street 1" value="{!newOnlineAcc.Billing_Street_1__c}"/>
                                <apex:outputField label="Street 2" value="{!newOnlineAcc.Billing_Street_2__c}"/>
                                <apex:outputField label="County" value="{!newOnlineAcc.Billing_County__c}"/>
                                <apex:outputField label="City" value="{!newOnlineAcc.Billing_City__c}"/>
                                <apex:outputField label="State" value="{!newOnlineAcc.Billing_State__c}"/>
                                <apex:outputField label="Zip Code" value="{!newOnlineAcc.Billing_Zip_Code__c}"/> 
                            </apex:pageBlocksection>
                            <apex:pageBlocksection columns="1">
                                <apex:outputField label="Phone" value="{!newOnlineAcc.Billing_Phone__c}"/>
                                <apex:outputField label="Fax" value="{!newOnlineAcc.Billing_Fax__c}"/>
                                <apex:outputField label="Email" value="{!newOnlineAcc.Billing_Email__c}"/>
                            </apex:pageBlocksection>
                        </apex:pageBlocksection>
                        <apex:pageBlocksection id="ship_info" title="{! IF(newOnlineAcc.New_Shipping_Location__c,'Shipping Information' ,'Shipping Address')}" columns="2">
                            <apex:pageBlocksection columns="1"> 
                                <!-- <apex:outputText value="{!newOnlineAcc.Business_Name__c}" label="Business Name" rendered="{!newOnlineAcc.New_Shipping_Location__c}"  />  -->
                                <apex:outputText value="{!newOnlineAcc.Customer_Type__c}" label="Customer Type" rendered="{!newOnlineAcc.New_Shipping_Location__c}"  />
                                <apex:outputText value="{!newOnlineAcc.Customer_Type_Other__c}" label="If other, please describe:" rendered="{!newOnlineAcc.New_Shipping_Location__c && newOnlineAcc.Customer_Type__c == 'Other'}"  />
                                <apex:outputText label="Street 1" value="{!newOnlineAcc.Shipping_Street_1__c}"/>
                                <apex:outputtext label="Street 2" value="{!newOnlineAcc.Shipping_Street_2__c}"/>
                                <apex:outputText label="County" value="{!newOnlineAcc.Shipping_County__c}"/>
                                <apex:outputText label="City" value="{!newOnlineAcc.Shipping_City__c}"/>
                                <apex:outputText label="State" value="{!newOnlineAcc.Shipping_State__c}"/>
                                <apex:outputText label="Zip Code" value="{!newOnlineAcc.Shipping_Zip_Code__c}"/>
                            </apex:pageBlocksection>
                            <apex:pageBlocksection columns="1"> 
                                <apex:outputText label="Phone" value="{!newOnlineAcc.Shipping_Phone__c}"/>
                                <apex:outputText label="Fax" value="{!newOnlineAcc.Shipping_Fax__c}"/>
                                <apex:outputText label="Email" value="{!newOnlineAcc.Shipping_Email__c}"/>
                            </apex:pageBlocksection> 
                        </apex:pageBlocksection>
                        <br/> 
                        <apex:pageBlockSection columns="1" >                            
                            <apex:outputText value="<font color=#4a4a56><b> Total number of practitioners at this location: </b></font> {!newOnlineAcc.Total_number_of_practitioners__c}" escape="false" rendered="{!newOnlineAcc.New_Shipping_Location__c}" />
                            <apex:outputText value="<font color=#4a4a56><b> Business Permit /License Number to operate within your state?: </b></font> {!newOnlineAcc.Business_Permit_or_License_Number__c}" escape="false" rendered="{!newOnlineAcc.New_Shipping_Location__c}"  />
                            <apex:outputText value="<font color=#4a4a56><b> Is there currently an account with Zoetis at this location?: </b></font> {!newOnlineAcc.Existing_account_with_Zoetis_at_this_Loc__c}" escape="false" />
                            <apex:outputText value="<font color=#4a4a56><b> Has there been a change in ownership?: </b></font> {!newOnlineAcc.Ownership_Change__c} "  escape="false" />
                            <apex:outputText value="<font color=#4a4a56><b> Date of Ownership Change: </b></font> {!newOnlineAcc.Date_of_Ownership_Change__c} " rendered="{!newOnlineAcc.Ownership_Change__c == 'Yes'}" escape="false"  />
                            <apex:outputText value="<font color=#4a4a56><b> Do you have an existing account with Zoetis?: </b></font> {!newOnlineAcc.Existing_account_with_Zoetis__c}" escape="false" rendered="{!!newOnlineAcc.New_Shipping_Location__c}" />
                            <apex:outputText value="<font color=#4a4a56><b> Account Name: </b></font> {!newOnlineAcc.Existing_Account_Name__c}"  rendered="{!newOnlineAcc.Existing_account_with_Zoetis__c=='Yes'}" escape="false"  />
                            <apex:outputText value="<font color=#4a4a56><b> Account#: </b></font> {!newOnlineAcc.Zoetis_Account__c}"  rendered="{!newOnlineAcc.Existing_account_with_Zoetis__c=='Yes'}" escape="false"  />
                            
                        </apex:pageBlockSection>  
                        <br/><label><b>Sales Tax Status: </b> {!newOnlineAcc.Sales_Tax_Status__c} </label> 
                        <apex:pageBlocksection columns="1" rendered="{!newOnlineAcc.Sales_Tax_Status__c=='Tax Exempt'}">
                            <apex:outputText value="<b>Tax Permit Number: </b> {!newOnlineAcc.Tax_Permit__c}" escape="false" />
                            <!-- <apex:outputText value="<font color=#4a4a56><b> Uploaded Tax File: </b></font> {!existingTaxAtt.Name}"  rendered="{!existingTaxAtt.Name != null}" escape="false"  /> -->
                        </apex:pageBlocksection> 
                        <br/>
                        
                    </apex:outputPanel>                       
                    <!-- 6. NOA_DiagnosticsorReproductiveServices  --> 
                    <apex:outputPanel rendered="{!formType == 'Diagnostics' }" >
                        <apex:pageBlocksection id="diaginfo" columns="1" title="Applicant Information">                            
                            <apex:outputField value="{!newOnlineAcc.FirstName}"  label="Applicant First Name" />
                            <apex:outputField value="{!newOnlineAcc.LastName}"  label="Applicant Last Name" />
                            <apex:outputField value="{!newOnlineAcc.Applicant_Role__c}"  label="Applicant Role" />
                            <apex:outputField value="{!newOnlineAcc.Email}"  label="Applicant Email" />
                            <apex:outputField value="{!newOnlineAcc.Phone}"  label="Applicant Phone" />
                            <apex:outputField value="{!newOnlineAcc.SAP_Bill_To_ID__c}" label="Bill To ID#"/>
                            <apex:outputField value="{!newOnlineAcc.Business_Name__c}"  label="Business Name" />
                            <apex:outputField value="{!newOnlineAcc.Customer_Type__c}" label="Customer Type" />
                            <apex:outputField value="{!newOnlineAcc.Customer_Sub_Type__c}" label="Customer Sub Type" />

                        </apex:pageBlocksection>  
                        <apex:pageBlocksection title="Billing Address" columns="2"  >
                            <apex:pageBlocksection columns="1" >
                                <apex:outputField label="Street 1" value="{!newOnlineAcc.Billing_Street_1__c}"/>
                                <apex:outputField label="Street 2" value="{!newOnlineAcc.Billing_Street_2__c}"/>
                                <apex:outputField label="County" value="{!newOnlineAcc.Billing_County__c}"/>
                                <apex:outputField label="City" value="{!newOnlineAcc.Billing_City__c}"/>
                                <apex:outputField label="State" value="{!newOnlineAcc.Billing_State__c}"/>
                                <apex:outputField label="Zip Code" value="{!newOnlineAcc.Billing_Zip_Code__c}"/> 
                            </apex:pageBlocksection>
                            <apex:pageBlocksection columns="1">
                                <apex:outputField label="Phone" value="{!newOnlineAcc.Billing_Phone__c}"/>
                                <apex:outputField label="Fax" value="{!newOnlineAcc.Billing_Fax__c}"/>
                                <apex:outputField label="Email" value="{!newOnlineAcc.Billing_Email__c}"/>
                            </apex:pageBlocksection>
                            <apex:pageBlockSection columns="1" >
                                <apex:outputText value="<font color=#4a4a56><b> I authorize Zoetis to send Invoices and Statements via: </b></font> {!newOnlineAcc.Contact_Preference__c}" escape="false" />
                            </apex:pageBlockSection>
                        </apex:pageBlocksection>
                        
                        <apex:pageBlocksection id="ship_info_diag" title="Shipping Address" columns="2">
                            <apex:pageBlocksection columns="1"> 
                                <apex:outputField label="Street 1" value="{!newOnlineAcc.Shipping_Street_1__c}"/>
                                <apex:outputField label="Street 2" value="{!newOnlineAcc.Shipping_Street_2__c}"/>
                                <apex:outputField label="County" value="{!newOnlineAcc.Shipping_County__c}"/>
                                <apex:outputField label="City" value="{!newOnlineAcc.Shipping_City__c}"/>
                                <apex:outputField label="State" value="{!newOnlineAcc.Shipping_State__c}"/>
                                <apex:outputField label="Zip Code" value="{!newOnlineAcc.Shipping_Zip_Code__c}"/>
                            </apex:pageBlocksection>
                            <apex:pageBlocksection columns="1"> 
                                <apex:outputField label="Phone" value="{!newOnlineAcc.Shipping_Phone__c}"/>
                                <apex:outputField label="Fax" value="{!newOnlineAcc.Shipping_Fax__c}"/>
                                <apex:outputField label="Email" value="{!newOnlineAcc.Shipping_Email__c}"/>
                            </apex:pageBlocksection> 
                            <br/><label><b>Sales Tax Status: </b> {!newOnlineAcc.Sales_Tax_Status__c} </label> 
                        </apex:pageBlocksection>
                        
                        <apex:pageBlocksection title="Type of Business" columns="1">
                            <apex:outputField label="Small Animal" value="{!newOnlineAcc.Small_Animal__c}"/>
                            <apex:outputField label="Equine" value="{!newOnlineAcc.Equine__c}"/>
                            <apex:outputField label="Beef" value="{!newOnlineAcc.Beef__c}"/>
                            <apex:outputField label="Dairy" value="{!newOnlineAcc.Dairy__c}"/>
                            
                            <apex:outputText value="<font color=#4a4a56><b> Type of Business: </b></font> {!newOnlineAcc.Type_of_Business__c}" escape="false" rendered="false" />
                            <apex:outputText value="<font color=#4a4a56><b> Business Other: </b></font> {!newOnlineAcc.Business_Other__c}" escape="false" rendered="false" />
                            
                            <apex:pageBlocksection columns="1" rendered="{!newOnlineAcc.Sales_Tax_Status__c=='Tax Exempt'}">
                                <apex:outputText value="<b>Tax Permit Number: </b> {!newOnlineAcc.Tax_Permit__c}" escape="false" />
                              <!--  <apex:outputText value="<font color=#4a4a56><b> Uploaded Tax File: </b></font> {!existingTaxAtt.Name}"  rendered="{!existingTaxAtt.Name != null}" /> -->
                            </apex:pageBlocksection>  
                        </apex:pageBlocksection>          
                        <br/>
                        <apex:pageBlockSection title="Banking and Trade Reference Information" columns="2" rendered="false" >
                            <apex:outputField style="white-space: nowrap;" value="{!newOnlineAcc.Principal_Bank_Name__c}"  label="Principal Bank Name" />
                            <apex:outputField value="{!newOnlineAcc.Principal_Bank_Phone__c}"  label="Principal Bank Phone" />
                            <apex:outputField value="{!newOnlineAcc.Trade_Reference_1_Company_Name__c}"  label="Trade References 1" />
                            <apex:outputField value="{!newOnlineAcc.Trade_Reference_1_Company_Phone__c}"  label="Phone" />
                            <apex:outputField value="{!newOnlineAcc.Trade_Reference_2_Company_Name__c}"  label="Trade References 2" />
                            <apex:outputField value="{!newOnlineAcc.Trade_Reference_2_Company_Phone__c}"  label="Phone" />
                            <apex:outputField value="{!newOnlineAcc.Trade_Reference_3_Company_Name__c}"  label="Trade References 3" />
                            <apex:outputField value="{!newOnlineAcc.Trade_Reference_3_Company_Phone__c}"  label="Phone" />
                        </apex:pageBlockSection>
                        <br/>
                        <!-- <apex:pageBlockSection columns="1" >
<apex:outputText value="<font color=#4a4a56><b> I authorize Zoetis to send Invoices and Statements via: </b></font> {!newOnlineAcc.Contact_Preference__c}" escape="false" />
<apex:outputText value="<font color=#4a4a56><b> Email: </b></font> {!newOnlineAcc.Authorization_Email__c}" escape="false" rendered="{!newOnlineAcc.Contact_Preference__c=='Email'}" />
</apex:pageBlockSection>  -->
                    </apex:outputPanel>
                    <br/>
                    
                     <!-- OwnershipInformation page Details -->
                        
                        <apex:pageBlocksection columns="1" title="Business Information"> 
                            <apex:outputText value="<font color=#4a4a56><b> Business Permit /License Number to operate within your state? : </b></font> {!newOnlineAcc.Business_Permit_or_License_Number__c}" escape="false" rendered="{!!newOnlineAcc.New_Shipping_Location__c}" />
                            <apex:outputText value="<font color=#4a4a56><b> Business Type: </b></font> {!newOnlineAcc.Ownership_Type__c}" escape="false" /> 
                            
                            <apex:pageBlocksection columns="2" rendered="{!newOnlineAcc.Ownership_Type__c=='Sole-proprietorship'}">
                                <apex:pageBlockSection columns="1">
                                    <apex:outputField value="{!newOnlineAcc.SP_Name__c}"  label="Name" /> 
                                    <apex:outputField value="{!newOnlineAcc.SP_Address__c}"  label="Address" />
                                    <apex:outputField value="{!newOnlineAcc.SP_County__c}"  label="County" />
                                    <apex:outputField value="{!newOnlineAcc.SP_City__c}"  label="City" />
                                    <apex:outputField value="{!newOnlineAcc.SP_State__c}"  label="State" />
                                    <apex:outputField value="{!newOnlineAcc.SP_Zip__c}"  label="Zip" />
                                </apex:pageBlockSection>
                                <apex:pageBlockSection columns="1"> 
                                    <apex:outputField value="{!newOnlineAcc.SP_Telephone__c}"  label="Telephone" /> 
                                    <apex:outputField value="{!newOnlineAcc.SP_Email__c}"  label="Email" />
                                    <apex:outputField value="{!newOnlineAcc.SP_Fax__c}"  label="Fax#" />
                                </apex:pageBlockSection> 
                            </apex:pageBlocksection>   
                            
                            <apex:pageBlocksection columns="2" rendered="{!newOnlineAcc.Ownership_Type__c=='Partnership'}">
                                <apex:pageBlockSection columns="1"> 
                                    <apex:outputField value="{!newOnlineAcc.Partership_Name__c}"  label="Name1" /> 
                                    <apex:outputField value="{!newOnlineAcc.Partnership_Title_or_Officer_Info_1__c}"  label="Title/Officer Information(1)" /> 
                                    <apex:outputField value="{!newOnlineAcc.Partership_Address__c}"  label="Address" /> 
                                    <apex:outputField value="{!newOnlineAcc.Partership_City__c}"  label="City" /> 
                                    <apex:outputField value="{!newOnlineAcc.Partership_County__c}"  label="County" /> 
                                    <apex:outputField value="{!newOnlineAcc.Partership_State__c}"  label="State" /> 
                                    <apex:outputField value="{!newOnlineAcc.Partership_Zip__c}"  label="Zip" /> 
                                    <apex:outputField value="{!newOnlineAcc.Partership_Telephone__c}"  label="Telephone#" /> 
                                    
                                    <apex:outputField value="{!newOnlineAcc.Partership_Fax__c}"  label="Fax#" />
                                </apex:pageBlockSection>
                                <apex:pageBlockSection columns="1">
                                    <apex:outputField value="{!newOnlineAcc.Partership_Name_2__c}"  label="Name2" />
                                    <apex:outputField value="{!newOnlineAcc.Partnership_Title_or_Officer_Info_2__c}"  label="Title/Officer Information(2)" />
                                    <apex:outputField value="{!newOnlineAcc.Partership_Address_2__c}"  label="Address" /> 
                                    <apex:outputField value="{!newOnlineAcc.Partership_City_2__c}"  label="City" /> 
                                    <apex:outputField value="{!newOnlineAcc.Partership_County_2__c}"  label="County" /> 
                                    <apex:outputField value="{!newOnlineAcc.Partership_State_2__c}"  label="State" /> 
                                    <apex:outputField value="{!newOnlineAcc.Partership_Zip_2__c}"  label="Zip" />
                                    <apex:outputField value="{!newOnlineAcc.Partership_Email__c}"  label="Email" />  
                                </apex:pageBlockSection>
                            </apex:pageBlocksection>
                        </apex:pageBlocksection>
                        
                        <apex:pageBlocksection columns="2" rendered="{!newOnlineAcc.Ownership_Type__c=='Corporation' || newOnlineAcc.Ownership_Type__c=='LLC' || newOnlineAcc.Ownership_Type__c=='Government'}">
                            <apex:outputField value="{!newOnlineAcc.CLG_Name__c}"  label="Name" />
                            <apex:outputField value="{!newOnlineAcc.CLG_Company__c}"  label="Company" />
                            <apex:outputField value="{!newOnlineAcc.CLG_Address__c}"  label="Address" /> 
                            <apex:outputField value="{!newOnlineAcc.CLG_City__c}"  label="City" /> 
                            <apex:outputField value="{!newOnlineAcc.CLG_County__c}"  label="County" /> 
                            <apex:outputField value="{!newOnlineAcc.CLG_State__c}"  label="State" />
